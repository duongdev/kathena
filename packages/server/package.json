{
  "name": "server",
  "version": "0.1.0",
  "description": "",
  "author": "Dustin Do <dustin.do95@gmail.com>",
  "private": true,
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "cross-env NODE_ENV=development nest start",
    "start:dev": "cross-env NODE_ENV=development nest start --watch",
    "start:debug": "cross-env NODE_ENV=development nest start --debug --watch",
    "start:prod": "cross-env NODE_ENV=production node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\"",
    "test": "cross-env NODE_ENV=test jest --config=./jest.config.js",
    "test:watch": "cross-env NODE_ENV=test jest --config=./jest.config.js --watch --runInBand",
    "test:cov": "cross-env NODE_ENV=test jest --config=./jest.config.js --coverage --runInBand --forceExit",
    "test:debug": "cross-env NODE_ENV=test node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "cross-env NODE_ENV=test jest --config ./test/jest-e2e.json"
  },
  "dependencies": {
    "@nestjs-modules/mailer": "1.6.0",
    "@nestjs/common": "8.2.1",
    "@nestjs/core": "8.2.1",
    "@nestjs/graphql": "7.11.0",
    "@nestjs/platform-express": "8.2.1",
    "@nestjs/platform-fastify": "8.2.1",
    "@nestjs/serve-static": "2.2.2",
    "@nestjs/testing": "8.2.1",
    "@typegoose/typegoose": "8.3.0",
    "@types/graphql-upload": "8.0.7",
    "apollo-server-express": "2.25.3",
    "apollo-server-fastify": "2.25.3",
    "bcrypt": "5.0.1",
    "body-parser": "1.19.0",
    "class-transformer": "0.4.0",
    "class-validator": "0.13.1",
    "cross-env": "7.0.3",
    "date-fns": "2.25.0",
    "dotenv": "10.0.0",
    "fastify-static": "4.5.0",
    "file-type": "16.5.3",
    "graphql": "15.7.2",
    "graphql-subscriptions": "2.0.0",
    "graphql-tools": "8.2.0",
    "graphql-upload": "12.0.0",
    "handlebars": "4.7.7",
    "jsonwebtoken": "8.5.1",
    "lodash": "4.17.21",
    "mongodb-memory-server": "6.9.6",
    "mongoose": "5.13.13",
    "nestjs-typegoose": "7.1.38",
    "nodemailer": "6.7.1",
    "reflect-metadata": "0.1.13",
    "rimraf": "3.0.2",
    "rxjs": "7.4.0",
    "shortid": "2.2.16",
    "source-map-support": "0.5.20",
    "type-graphql": "1.1.1"
  },
  "devDependencies": {
    "@nestjs/cli": "8.1.4",
    "@nestjs/schematics": "8.0.4",
    "@types/bcrypt": "5.0.0",
    "@types/date-fns": "2.6.0",
    "@types/dotenv": "8.2.0",
    "@types/express": "4.17.13",
    "@types/file-type": "10.9.1",
    "@types/jest": "27.0.2",
    "@types/jsonwebtoken": "8.5.6",
    "@types/lodash": "4.14.177",
    "@types/mongodb-memory-server": "2.3.0",
    "@types/node": "14.17.33",
    "@types/shortid": "0.0.29",
    "@types/supertest": "2.0.11",
    "@typescript-eslint/eslint-plugin": "5.4.0",
    "@typescript-eslint/parser": "5.4.0",
    "eslint": "7.32.0",
    "eslint-config-airbnb": "18.2.1",
    "eslint-config-airbnb-typescript": "15.0.0",
    "eslint-config-prettier": "8.3.0",
    "eslint-plugin-import": "2.25.3",
    "eslint-plugin-jest": "25.2.4",
    "eslint-plugin-prettier": "4.0.0",
    "jest": "27.3.1",
    "start-server-webpack-plugin": "2.2.5",
    "supertest": "6.1.6",
    "ts-jest": "27.0.7",
    "ts-loader": "9.2.6",
    "ts-node": "10.4.0",
    "tsconfig-paths": "3.11.0",
    "typescript": "4.1.2",
    "webpack-node-externals": "3.0.0"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      ".+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
